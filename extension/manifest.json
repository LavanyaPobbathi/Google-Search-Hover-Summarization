{
    "manifest_version": 3,
    "name": "Google Search Summarizer",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "http://localhost:5001/*",
      "https://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://www.google.com/search*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }